{
    "manifest_version": 3,
    "name": "EOP",
    "version": "2.0",
    "description": " A extension for HaUI Student",
    "permissions": [
      "scripting"],
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "64": "images/icon64.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "64": "images/icon64.png",
      "128": "images/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://sv.haui.edu.vn/*"],
        "js": ["scripts.js"]
      }
    ]
  }